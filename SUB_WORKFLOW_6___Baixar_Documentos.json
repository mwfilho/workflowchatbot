    {
      "parameters": {
        "jsCode": "// Consolidar downloads\nconst tipo = $node['Input Parameters'].json.tipo;\nconst numeroProcesso = $node['Input Parameters'].json.numeroProcesso;\n\n// Para download integral\nif (tipo === 'integral') {\n    const file = $input.first().binary;\n    return {\n        sucesso: true,\n        tipo: 'integral',\n        numeroProcesso: numeroProcesso,\n        arquivo: {\n            nome: `processo_${numeroProcesso}_integral.pdf`,\n            tamanho: file?.data?.length || 0,\n            formato: 'PDF',\n            disponivel: true\n        },\n        mensagem: 'Download integral realizado com sucesso'\n    };\n}\n\n// Para downloads mÃºltiplos\nconst arquivos = $input.all().map((item, index) => {\n    const binary = item.binary;\n    return {\n        nome: `documento_${index + 1}.pdf`,\n        tamanho: binary?.data?.length || 0,\n        disponivel: true\n    };\n});\n\nreturn {\n    sucesso: true,\n    tipo: tipo,\n    numeroProcesso: numeroProcesso,\n    arquivos: arquivos,\n    totalArquivos: arquivos.length,\n    mensagem: `Download de ${arquivos.length} documentos realizado com sucesso`\n};"
      },
