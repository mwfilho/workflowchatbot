    {
      "parameters": {
        "jsCode": "// Filtrar documentos principais\nconst documentos = $json.documentosPrincipais || [];\nconst numeroProcesso = $node['Input Parameters'].json.numeroProcesso;\n\nif (documentos.length === 0) {\n    throw new Error('Nenhum documento principal encontrado');\n}\n\n// Preparar lista de IDs para download\nconst idsParaDownload = documentos.map(doc => doc.id).filter(id => id);\n\nconsole.log(`ðŸ“„ Preparando download de ${idsParaDownload.length} documentos principais`);\n\nreturn {\n    numeroProcesso: numeroProcesso,\n    documentIds: idsParaDownload,\n    documentos: documentos,\n    tipo: 'principais'\n};"
      },
