      "parameters": {
        "jsCode": "// Processar documentos especÃ­ficos\nconst documentIds = $node['Input Parameters'].json.documentIds;\nconst numeroProcesso = $node['Input Parameters'].json.numeroProcesso;\n\nif (!documentIds || documentIds.trim() === '') {\n    throw new Error('IDs de documentos nÃ£o fornecidos');\n}\n\n// Separar IDs\nconst ids = documentIds.split(',').map(id => id.trim()).filter(id => id);\n\nif (ids.length === 0) {\n    throw new Error('Nenhum ID vÃ¡lido fornecido');\n}\n\nconsole.log(`ðŸ“„ Preparando download de ${ids.length} documentos especÃ­ficos`);\n\nreturn {\n    numeroProcesso: numeroProcesso,\n    documentIds: ids,\n    tipo: 'especificos'\n};"
